      "linebreak-style": 0,
      "no-console": 0,
      "comma-dangle": 0,
      "object-curly-newline": 0,
      "arrow-body-style": 0,
      "jsx-a11y/label-has-associated-control": 0
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "eslint-config-airbnb": "^18.2.1"
  }
